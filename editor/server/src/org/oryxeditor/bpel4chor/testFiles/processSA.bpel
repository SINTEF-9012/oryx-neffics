<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<process name="buyerref" targetNamespace="example:buyer" abstractProcessProfile="urn:HPI_IAAS:choreography:profile:2006/12" xmlns:wsu="http://schemas.xmlsoap.org/ws/2002/07/utility" >
<sequence>
  <receive wsu:id="ReceivePI" createInstance="yes" />
    <forEach wsu:id="RequestPriceFE" parallel="yes">
      <scope>
       <sequence>
         <invoke wsu:id="RequestPrice" />
         <receive wsu:id="ReceivePrice" />
       </sequence>
      </scope>
      <completionCondition name="testCondition" />
    </forEach>
    <opaqueActivity name="SelectAirline" />
    <invoke wsu:id="OrderTickets" />
    <receive wsu:id="ReceiveConf" />
    <opaqueActivity name="CreateItinerary" />
    <invoke wsu:id="IssueItinerary" />
    <correlationSets>
      <correlationSet name="correlationSetName1" properties="ProcessID InstanceID" />
      <correlationSet name="correlationSetName2" properties="buyerref" />
      <correlationSet name="correlationSetName3" />
    </correlationSets>
</sequence>
</process>

